package Giris;

import java.util.Scanner;
public class patika {
    public static void main (String[] args) {

        double boy, kilo, vki;
        Scanner inp = new Scanner(System.in);

        System.out.print("Lütfen kilonuzu giriniz : ");
        kilo = inp.nextDouble();

        System.out.print("Lütfen boy'unuzu giriniz : ");
        boy = inp.nextDouble();

        vki = kilo / (boy * boy);

        System.out.println("VKİ oranınız : " + vki);




    }
}
